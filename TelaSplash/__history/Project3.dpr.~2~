program Project3;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {frm_principal},
  Unit2 in 'Unit2.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(Tfrm_principal, frm_principal);
  Application.CreateForm(TForm2, Form2);
  Application.Run;
end.
